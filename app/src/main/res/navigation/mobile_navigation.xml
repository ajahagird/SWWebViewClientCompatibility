<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.swwebviewclientcompaibilitydemo.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">

        <argument
            android:name="url"
            app:argType="string"
            android:defaultValue="https://unruly-zest-blossom.glitch.me"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_200"
        android:name="com.example.swwebviewclientcompaibilitydemo.HomeFragment"
        android:label="@string/title_SW_200"
        tools:layout="@layout/fragment_notifications">

        <argument
            android:name="url"
            app:argType="string"
            android:defaultValue="https://unruly-zest-blossom.glitch.me"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_nonsw_200"
        android:name="com.example.swwebviewclientcompaibilitydemo.HomeFragment"
        android:label="@string/title_NonSW_200"
        tools:layout="@layout/fragment_notifications">

        <argument
            android:name="url"
            app:argType="string"
            android:defaultValue="https://unruly-zest-blossom.glitch.me?nosw=1"/>
    </fragment>

</navigation>